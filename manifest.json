{
  "name": "__MSG_extName__",
  "version": "2012.8.11.1",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "images/angle.png",
    "48": "images/angle.png",
    "128": "images/angle.png"
  },
  "page_action": {
    "default_icon": "images/angle.png",
    "default_title": "__MSG_availableCharacters__"
